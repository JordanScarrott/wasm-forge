{
  "name": "wasm-forge",
  "version": "0.2.0",
  "private": false,
  "sideEffects": false,
  "description": "High-performance WebAssembly utilities for modern JavaScript.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build"
  },
  "dependencies": {
    "@wasm-forge/trie": "workspace:*",
    "@wasm-forge/levenshtein": "workspace:*",
    "@wasm-forge/merge-sort": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.5.3",
    "vite-plugin-wasm": "^3.2.2"
  }
}
